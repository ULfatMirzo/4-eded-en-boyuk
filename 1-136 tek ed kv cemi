using System;

class Program
{
    static void Main()
    {
        int sum = 0;
        for (int i = 1; i <= 136; i++)
        {
            if (i % 2 != 0)
            {
                sum += i * i;
            }
        }
        Console.WriteLine("1-136 arası tək ədədlərin kvadratları cəmi: " + sum);
    }
}
