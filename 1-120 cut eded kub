using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("1-120 arası cüt ədədlərin kubu:");
        for (int i = 1; i <= 120; i++)
        {
            if (i % 2 == 0)
            {
                Console.WriteLine($"{i}^3 = {i * i * i}");
            }
        }
    }
}
