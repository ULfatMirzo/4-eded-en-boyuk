using System;

class Program
{
    static void Main()
    {
        Console.Write("3 rəqəmli ədəd daxil edin: ");
        int number = int.Parse(Console.ReadLine());

        int a = number / 100;
        int b = (number / 10) % 10;
        int c = number % 10;

        if (a == b || b == c || a == c)
            Console.WriteLine("Ən azı iki rəqəm eynidir.");
        else
            Console.WriteLine("Rəqəmlər fərqlidir.");
    }
}
